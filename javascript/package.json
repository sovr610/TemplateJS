{
  "name": "homefront-api",
  "version": "1.0.0",
  "description": "cynerge homefront nodejs backend.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/mocha.js",
    "build": "gulp",
    "startDev": "node src/app.js",
    "startProd": "node publish/main-min.js",
    "pretty": "prettier --write --tab-width 2 \"src/**/*.js\"",
    "swagger": "swagger-jsdoc -d swaggerDef.js -o ./src/app/swagger.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "git add"
    ]
  },
  "keywords": [
    "node",
    "js",
    "cynerge",
    "homefront",
    "api"
  ],
  "author": "Parker Bidigare",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "chai": "^4.2.0",
    "del": "^5.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-docgen": "^0.4.3",
    "eslint-plugin-jsdoc": "^28.6.1",
    "eslint-plugin-markdown": "^2.0.0-alpha.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-xss": "^0.1.10",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-bundle-assets": "^2.29.0",
    "gulp-concat": "^2.6.1",
    "gulp-json": "^0.1.4",
    "gulp-minify": "^3.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "lint-staged": "^10.2.11",
    "mocha": "^10.2.0",
    "sonarjs": "^1.0.0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "eslint-config-standard": "^14.1.1",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-rename": "^2.0.0",
    "helmet": "^3.23.3",
    "husky": "^4.2.5",
    "pino-pretty": "^4.0.0",
    "redrun": "^8.0.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-dist": "^3.28.0"
  }
}
